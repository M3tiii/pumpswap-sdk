{
  "name": "@pump-fun/pump-swap-sdk",
  "version": "0.0.1-beta.39",
  "description": "Official SDK for interacting with Pump Swap AMM protocol on Solana",
  "main": "./dist/index.js",
  "module": "./dist/esm/index.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/esm/index.js"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "tsup src/index.ts --clean --dts",
    "clean": "rimraf dist",
    "prebuild": "npm run clean",
    "prepare": "npm run build",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "prepublishOnly": "npm run build",
    "typecheck": "tsc --noEmit"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pump-fun/pump-swap-sdk.git"
  },
  "keywords": [
    "pump-swap",
    "pump.fun",
    "solana",
    "web3",
    "defi",
    "amm",
    "sdk",
    "cryptocurrency"
  ],
  "author": {
    "name": "Pump Fun",
    "email": "dev@pump.fun",
    "url": "https://pump.fun"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/pump-fun/pump-swap-sdk/issues"
  },
  "homepage": "https://docs.pump.fun",
  "engines": {
    "node": ">=25"
  },
  "devDependencies": {
    "@babel/preset-env": "7.28.5",
    "@babel/preset-typescript": "7.28.5",
    "@types/bn.js": "5.2.0",
    "@types/chai": "4.3.20",
    "@types/jest": "29.5.14",
    "@types/node": "20.10.0",
    "@typescript-eslint/eslint-plugin": "8.46.3",
    "@typescript-eslint/parser": "8.46.3",
    "chai": "4.5.0",
    "eslint": "8.57.1",
    "eslint-config-prettier": "9.1.0",
    "jest": "29.7.0",
    "prettier": "3.6.2",
    "rimraf": "5.0.9",
    "ts-jest": "29.4.5",
    "ts-node": "10.9.2",
    "tsup": "8.5.0",
    "typescript": "5.9.3"
  },
  "dependencies": {
    "@coral-xyz/anchor": "0.32.1",
    "@solana/spl-token": "0.4.14",
    "@solana/web3.js": "1.98.4"
  }
}
